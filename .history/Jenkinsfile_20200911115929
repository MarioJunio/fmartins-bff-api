pipeline {
    agent any

    stages {

        stage("build") {

            steps {
                echo "Fazendo build do projeto..."
            }
        }

        stage("test") {

            steps  {
                echo "Fazendo test do projeto..."
            }
        }

        stage("deploy") {

            steps {
                echo "Realizando deploy na aws..."
            }
        }
    }
}